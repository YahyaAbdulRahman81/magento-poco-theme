                                                    </item>
                                                    <item name="children" xsi:type="array">
														<item name="newsletter-subscribe" xsi:type="array">
                                                            <item name="component" xsi:type="string">Magento_Ui/js/form/form</item>
                                                            <item name="displayArea" xsi:type="string">newsletter-subscribe</item>
                                                            <item name="config" xsi:type="array">
                                                                <item name="template" xsi:type="string">Magebees_Onepagecheckout/form/element/newsletter-subscribe</item>
                                                          </item>
                                                        </item>
