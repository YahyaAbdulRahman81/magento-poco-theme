<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
		<resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
<resource id="Magebees_Blog::heading" title="MageBees Blog" sortOrder="0">	
				<resource id="Magebees_Blog::post" title="Post" sortOrder="10" />
				<resource id="Magebees_Blog::category" title="Category" sortOrder="20" />
		                <resource id="Magebees_Blog::tag" title="Tags" sortOrder="30" />
				<resource id="Magebees_Blog::comment" title="Comments" sortOrder="40" />
				<resource id="Magebees_Blog::import" title="Import" sortOrder="50" />
				<resource id="Magebees_Blog::urlrewrite" title="Import" sortOrder="60" />
				</resource>
				</resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magebees_Blog::blogsetting" title="Blog Setting"  />
                
                    </resource>
                    </resource>
                </resource>
				
             </resource>
        </resources>
    </acl>
</config>
