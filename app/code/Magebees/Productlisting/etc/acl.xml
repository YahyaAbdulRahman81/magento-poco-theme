<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
		<resources>
			<resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magebees_Productlisting::configuration_settings" title="Product Listing Settings" sortOrder="10000" />
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Catalog::catalog">
                   <resource id="Magebees_Productlisting::productlisting_content" title="Product Listing" sortOrder="50">
				</resource>
                </resource>
            </resource>
	   </resources>
    </acl>
</config>

