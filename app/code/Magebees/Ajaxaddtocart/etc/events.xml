<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <!--Dispatch event and set content for dispatched event-->
    
    <event name="ajaxcheckout_cart_add_product_complete">
        <observer name="magebees_addtocart" instance="Magebees\Ajaxaddtocart\Observer\Addtocart" />
    </event>
    <event name="wishlist_cart_add_product_complete">
        <observer name="wishlist_addtocart" instance="Magebees\Ajaxaddtocart\Observer\WishlistAddtocart" />
    </event>
    <event name="checkout_cart_delete_product_complete">
        <observer name="magebees_deletecart" instance="Magebees\Ajaxaddtocart\Observer\RemoveCart" />
    </event>
    <event name="controller_action_postdispatch_wishlist_index_configure">
        <observer name="wishlist_configure" instance="Magebees\Ajaxaddtocart\Observer\WishlistConfigure" />
    </event>
    
</config>
